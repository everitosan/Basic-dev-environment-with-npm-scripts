{
  "name": "interative_js",
  "version": "1.0.0",
  "description": "Examples of interactive js",
  "main": "index.js",
  "scripts": {
    "sass": "./bin/sass",
    "babel": "./bin/babel",
    "browserify": "./bin/browserify",
    "lint": "./bin/lint",
    "jade": "./bin/jade",
    "images": "./bin/imagemin",
    "serve": "cd app/dist && browser-sync start --server --files 'css/*.css, js/*.js, *.html'",
    "build:js": "npm run lint && npm run browserify",
    "watch:css": "onchange 'app/sass/**/*.sass' -- npm run sass",
    "watch:js": "onchange 'app/js/**/*.js' -- npm run build:js",
    "watch:jade": "onchange 'app/**/*.jade' -- npm run jade",
    "develop": "npm-run-all --parallel serve watch:*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "jade-cli": "^0.1.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.6.0",
    "onchange": "^2.1.2"
  }
}
